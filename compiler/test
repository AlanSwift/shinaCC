int binarySearch(int *a, int len, int key);


int main()
{
	int a[10];
	int i, j;
	char *c;
	int (*fp)(int*, int, int);
	fp = binarySearch;
	i = 0;
	c = "hello world";
	if(a[i] < 10 || a[i] > 20)
		a[i] = 1;
	for(i = 1; i <= 10; i += 1)
		for(j = 1; j <= 20; j += 1)
			a[i] = i + j;
	for(i = 0; i < 10; i += 1){
		a[i] = 100 - i;
	}
	i = fp(a, 10, 91);
	i = binarySearch(a, 10, 91);
	return 0;
}


int binarySearch(int *a, int len, int key)
{
	int left, right, mid;
	left = 0, right = len - 1;
	while(left <= right){
		mid = (left + right) / 2;
		if(a[mid] == key)
			return mid;
		else if(a[mid] < key)
			left = mid + 1;
		else
			right = mid - 1;
	}
	return -1;
}