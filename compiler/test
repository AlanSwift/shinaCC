int g(int a)
{
	return a * a;
}

int f(int a, int b)
{
	int c = g(a);
	c += g(b);
	return g(c);
}


int main()
{
	int a[2];
    long c;
    double d;
    int (**gp)(int, int);
    long *p = &c;
    a[0] = 1;
    a[1] = 2;
    while(a[0] <= 10){
        a[1] += a[0];
        a[0]++;
    }
    *p = f(a[0], a[1]);
    printf("c=%ld\n", c);
    return 0;
}