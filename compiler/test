void merge(int *a, int *b, int *c, int len)
{
	int aptr, bptr, ptr, i;
	aptr = 0;
	bptr = 0;
	ptr = 0;
	while(aptr < len && bptr < len){
		if(a[aptr] < b[bptr]){
			c[ptr] = a[aptr];
			aptr += 1;
		}
		else{
			c[ptr] = b[bptr];
			bptr += 1;
		}
		ptr += 1;
	}
}


int main()
{
	int a[10], b[10], c[20];
	int i, t, j;
	char *cp;
	cp = "result = %d\n";
	cp = "center = %d\n";
	a[0]=0;
	a[1]=1;
	a[2]=2;
	a[3]=3;
	a[4]=4;
	a[5]=5;
	a[6]=6;
	a[7]=7;
	a[8]=8;
	a[9]=9;

	b[0]=1;
	b[1]=11;
	b[2]=21;
	b[3]=31;
	b[4]=41;
	b[5]=51;
	b[6]=61;
	b[7]=71;
	b[8]=81;
	b[9]=91;
	merge(a, b, c, 5);
	for(i = 0; i < 20; i += 1){
		t = c[i];
	}	
	return 0;
}